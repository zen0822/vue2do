{
  "name": "vue2do",
  "version": "1.0.0",
  "description": "Vue2do Org.",
  "author": "Zheng (Zen) Huang (Wong) <zenwongg@aliyun.com>",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "app:i": "cd ./app/doc && yarn install && cd ../mock && yarn install",
    "coveralls": "echo \"No test specified\" && exit 0",
    "debug": "node --inspect-brk=5858 bin/dev.js",
    "dev:D": "vue2do-build dev app/doc/project.config.js",
    "dev:E": "vue2do-build dev app/ex/project.config.js",
    "dev:M": "yarn run tsc && cross-env NODE_ENV=development vue2do-build dev ./tsDist/app/mock/project.config.js --root ./app/mock/",
    "gql:gen": "graphql-codegen --config codegen.yml",
    "lerna": "lerna",
    "lerna:i": "lerna exec -- yarn install",
    "lerna:rel": "lerna publish",
    "lerna:test": "lerna run test",
    "lerna:tsc": "lerna run tsc",
    "mock.gql:M": "lerna run tsc && cross-env APOLLO_ENGINE_KEY=service:mockServer:LPVLKoXjQSJZfFYIl19zzw && vue2do-mock gql ./tsDist/app/mock/mock.config.js --root ./app/mock/",
    "mock.sw.dev:M": "lerna run tsc && cross-env NODE_ENV=development vue2do-mock sw dev ./tsDist/app/mock/mock.config.js --root ./app/mock/",
    "mock.sw.prod:M": "lerna run tsc && vue2do-mock sw prod ./tsDist/app/mock/mock.config.js --root ./app/mock/",
    "prod:D": "vue2do-build prod app/doc/project.config.js",
    "prod:M": "yarn run tsc && cross-env NODE_ENV=production vue2do-build prod ./tsDist/app/mock/project.config.js",
    "prod.rel:D": "node ./app/doc/script/prod.js && apollo service:push",
    "prod.rel.ci:D": "node ./app/doc/script/prod-ci.js",
    "prod:E": "vue2do-build prod app/example/project.config.js",
    "prisma:db": "docker-compose up -d",
    "prisma:start": "prisma deploy",
    "test:D": "vue2do-test unit ./app/doc/test.config.js",
    "test.ci:D": "node ./app/doc/script/test.ci.js",
    "tsc": "tsc",
    "tsc:w": "tsc -w"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.26",
    "@types/bcryptjs": "^2.4.2",
    "@types/chai": "^4.2.9",
    "@types/jsonwebtoken": "^8.3.7",
    "@types/mocha": "^7.0.1",
    "@types/sinon": "^7.5.2",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "@vue/composition-api": "^0.4.0",
    "@vue/test-utils": "^1.0.0-beta.31",
    "bcryptjs": "^2.4.3",
    "chai": "^4.2.0",
    "core-js": "^3.3.3",
    "dexie": "^2.0.4",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-jsx": "^0.1.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "extract-loader": "^4.0.3",
    "husky": "^3.0.9",
    "jsonwebtoken": "^8.5.1",
    "lerna": "^3.20.2",
    "lint-staged": "^9.4.2",
    "micromatch": "^4.0.2",
    "prisma-client-lib": "^1.34.0",
    "regenerator-runtime": "^0.13.3",
    "stylelint": "^11.0.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-scss": "^3.11.1",
    "vue": "^2.6.11",
    "vue-class-component": "^7.1.0",
    "vue-i18n": "^8.3.0",
    "vue-property-decorator": "^8.1.1",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.2",
    "webpack": "^4.41.2"
  },
  "dependencies": {
    "@commitlint/config-conventional": "^8.2.0",
    "@types/ws": "^7.2.1",
    "@vue2do/build": "^1.1.13",
    "@vue2do/component": "^1.1.14",
    "@vue2do/test": "^1.1.16",
    "chalk": "^3.0.0",
    "commitlint": "^8.2.0",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "prisma": "^1.34.10",
    "typescript": "^3.8.3",
    "write": "^2.0.0"
  }
}
