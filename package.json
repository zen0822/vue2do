  "scripts": {
    "apollo": "apollo",
    "apollo:engine": "apollo service:push",
    "coveralls": "cat test/coverage/lcov.info | coveralls",
    "doc": "node bin/doc",
    "doc:rel": "node bin/doc --release && apollo service:push",
    "doc:rel:ci": "node bin/doc --release --ci && apollo service:push",
    "gql": "npm run tsc && cross-env APOLLO_ENGINE_KEY=service:mockServer:LPVLKoXjQSJZfFYIl19zzw nodemon --watch ./script/gql.ts --watch ./tsDist/example/server/gql bin/gql.js",
    "gql:gen": "graphql-codegen --config codegen.yml",
    "mock:sw": "nodemon --watch ./script/sw.dev.js --watch ./script/config/sw.dev.webpack.conf.js bin/sw.dev.js",
    "prisma:db": "docker-compose up -d",
    "prisma:start": "prisma deploy",
    "prod": "node bin/prod",
    "rel": "bash script/release.sh",
    "start": "nodemon --watch ./script/* --ignore ./script/config/sw.dev.webpack.conf.js bin/dev.js",
    "test": "cross-env BABEL_ENV=testing karma start test/karma",
    "test:ci": "cross-env BABEL_ENV=testing karma start test/karma.ci",
    "test:unit": "cross-env NODE_ENV=testing jest",
    "tsc": "tsc",
    "tsc:watch": "tsc -w"
  },
